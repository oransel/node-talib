  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "scripts": {
    "build": "node ./src/lib/build.js && node-gyp configure && node-gyp build",
    "build-prebuild": "prebuild --all",
    "install": "prebuild-install --verbose || node-gyp rebuild",
    "clean": "node ./src/lib/clean.js"
  },
  "main": "build/Release/talib.node",
  "engines": {
    "node": ">=8.16.0"
  },
  "dependencies": {
    "nan": "^2.13.2",
    "prebuild-install": "^5.3.2"
  },
  "devDependencies": {
    "prebuild": "^10.0.0"
